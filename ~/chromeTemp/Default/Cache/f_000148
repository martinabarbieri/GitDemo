<script type="text/javascript">

	function closeMenu() {
		$('.dropdown-menu').stop(true, true).slideUp("fast");
		$('.dropdown-menu').toggleClass('open');
	}

	$(document).ready(function () {
		$(".dropdown").hover(
			function () {
				$('.dropdown-menu', this).stop(true, true).slideDown("fast");
				$(this).toggleClass('open');
			},
			function () {
				$('.dropdown-menu', this).stop(true, true).slideUp("fast");
				$(this).toggleClass('open');
			}
		);

		/* $(".divider-last").blur(
			function() {
				console.log("Blur Triggered");
				$('.dropdown-menu', this.parentElement.parentElement.parentElement).stop( true, true ).slideUp("fast");
				$(this.parentElement.parentElement.parentElement).removeClass('open');
			}
		); */

		$("#inputheaderlg").focus(
			function () {
				closeMenu();
			}
		);

		$("#accedi").focus(
			function () {
				closeMenu();
			}
		);

		$("#home_link").focus(
			function () {
				closeMenu();
			}
		);

		$("#header_chart").focus(
			function () {
				closeMenu();
			}
		);

		$("#header_messages").focus(
			function () {
				closeMenu();
			}
		);

		$("#go_to_mef_link").focus(
			function () {
				closeMenu();
			}
		);

		$(".dropdown-toggle").focus(
			function () {
				//console.log("Focus Triggered");
				$('.dropdown-menu').stop(true, true).slideUp("fast");
				$('.dropdown-menu').toggleClass('open');
				$('.dropdown-menu', this.parentElement).stop(true, true).slideDown("fast");
				$(this).toggleClass('open');
				/*var menuLinkColl = this.parentElement.children[this.parentElement.children.length - 1].children
				var lastMenuLink = menuLinkColl[menuLinkColl.length - 1].children[0];
				var theMenu = this.nextElementSibling; */
				/*lastMenuLink.blur(
					function() {
						console.log("Blur Triggered");
						theMenu.stop(true, true).slideUp("fast");
						theMenu.toggleClass('open');
					}
				); */
			}
		);


		/* $(".dropdown-toggle").blur(
			function() {
				console.log("Blur Triggered");
				$('.dropdown-menu', this.parentElement).stop( true, true ).slideUp("fast");
				$(this).toggleClass('open');
			}
		); */

		$('.nav li:has(ul)').doubleTapToGo();
	});
</script>
